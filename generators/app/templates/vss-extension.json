{
    "manifestVersion": 1,
    "version": "1.0.0",
    "name": "<%= name %>",
    "description": "<%= description %>",
    "publisher": "<%= publisher %>",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "demands": [],
    "categories": ["Azure Pipelines"],
    "icons": {
        "default": "images/logo.png"
    },
    "tags": ["<%= name %>"],
    "scopes": [],
    "content": {
        "details": {
            "path": "static/overview.md"
        },
        "license": {
            "path": "static/apache-v2.0.md"
        }
    },
    "links": {
        "getstarted": {
            "uri": "https://www.<%= publisher %>.com/help/getstarted"
        },
        "support": {
            "uri": "https://www.<%= publisher %>.com/support"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/<%= publisher %>/<%= name %>"
    },
    "branding": {
        "color": "rgb(34, 34, 34)",
        "theme": "dark"
    },
    "screenshots": [
        {
            "path": "images/screen1.png"
        }
    ],
    "badges": [],
    "files": [
        {
            "path": "images",
            "addressable": true
        },
        {
            "path": "static",
            "addressable": true
        }
    ],
    "contributions": []
}
